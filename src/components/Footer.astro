---
interface Props {
  title: string;
  message: string;
  github: string;
  twitter: string;
}

const {  title, message, github, twitter  }  = Astro.props;
---
<footer>
    <p>{title}</p>
    <p>{message}</p>
    <div class="social">
      <a href={github}
        ><i class="fab fa-github"></i
      ></a>
      <a href={twitter}
        ><i class="fab fa-twitter"></i
      ></a>
    </div>
    <p class="end">CopyRight@{title}</p>
</footer>